{
  "name": "api-basic",
  "version": "1.0.0",
  "description": "Api Rest Full Dynamic.",
  "main": "dist/server.js",
  "scripts": {
    "start": "pnpm build && node dist/server.js",
    "build": "tsc && echo \"✅ Proceso realizado con éxito.\"",
    "dev": "pnpm build && concurrently \"tsc -w\" \"nodemon dist/server.js\"",
    "watch": "pnpm build && concurrently \"tsc -w\" \"node --watch dist/server.js\"",
    "prod": "SET NODE_ENV=production && pnpm start",
    "vercel": "vercel ls && echo \"✅ Proceso realizado con éxito.\"",
    "publish": "pnpm build && pnpm vercel"
  },
  "keywords": [
    "backend",
    "api",
    "api basic",
    "express",
    "node",
    "typescript"
  ],
  "author": {
    "name": "Dc Dev -> David Coach Dev.",
    "email": "dcdevtk@gmail.com",
    "url": "https://dcdev.tk"
  },
  "license": "MIT ",
  "dependencies": {
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
