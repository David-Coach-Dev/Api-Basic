{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "dist/**/*.js"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/api"
    }
  ],
  "ignoreFiles": [
    "node_modules",
    "response",
    ".hl",
    ".vscode",
    "mysql"
  ]
}