{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "name": "Api Rest Full Dynamic",
  "builds": [
    {
      "src": "dist/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.js"
    }
  ],
  "redirects": [
    {
      "source": "/docs",
      "destination": "/docs/",
      "permanent": false
    },
    {
      "source": "/",
      "destination": "/api",
      "permanent": false
    }
  ]
}
