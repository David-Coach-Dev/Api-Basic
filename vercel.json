{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "name": "Api Rest Full Dynamic",
  "builds": [
    {
      "src": "dist/server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "dist/**/**/*.js",
          "dist/*.js"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/server.js"
    },
    {
      "src": "/docs/(.*)",
      "dest": "dist/server.js"
    }
  ]
}