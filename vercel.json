{
  "name": "api-back-v-1",
  "version": 2,
  "installCommand": "npm install",
  "trailingSlash": false,
  "public": true,
  "env": {
    "MY_KEY": "this is the value",
    "SECRET": "@my-secret-name"
  },
  "builds": [
    {
      "src": ["package.json"],
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "methods": [
        "GET, POST, PUT, DELETE"
      ],
      "dest": "api/server.ts",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET,POST,PUT,DELETE",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    },
    {
      "src": "/(.*)",
      "dest": "api/$1"
    }
  ]
}