{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "build": {
    "public": "dist",
    "command": "pnpm build",
    "env": {
      "NODE_ENV": "production"
    }
  },
  "routes": [
    {
      "src": "/[^.]+",
      "dest": "/",
      "status": 200,
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "OPTION"
      ]
    }
  ]
}