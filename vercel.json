{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "/dist/server.js",
      "permanent": false
    },
    {
      "source": "/docs",
      "destination": "/docs/",
      "permanent": false
    },
    {
      "source": "/",
      "destination": "/api",
      "permanent": false
    }
  ]
}
