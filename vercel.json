{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/server.js"
    }
  ],
  "redirects": [
    {
      "source": "/docs",
      "destination": "/docs/",
      "permanent": false
    },
    {
      "source": "/",
      "destination": "/api",
      "permanent": false
    }
  ]
}
